#-------------- Image Aspect Ratio---------------#
$width = $imgScale*1500;
$height = $imgScale*1500;


#---- Provided --------#
# random(lower, upper, stepsize)
$f = random(100,1000,100);
$p = random(100,400,100);
$d1 = random(3,5,1);
$d2 = random(1,3,0.5);
$d3 = random(1,3,0.5);
$hyp = sqrt(($d2+$d3)**2 + ($d1)**2);

#---Reaction Forces---
$r = $p * $d2;
$d = $d2/2;

