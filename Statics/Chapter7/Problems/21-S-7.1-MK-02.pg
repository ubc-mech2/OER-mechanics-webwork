#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*1700;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)
$f1 = 12;
$f2 = 9;
$d1 = 1.5;
$d2 = 1.5;
$d3 = 1.5;
$d4 = 1.5;
$d = $d3 + $d4;

#---Reaction Forces---
$fd2 = $f2 * $d;
$Ay = ($fd2 * $d4 + $f1 * ($d2 + $d3 + $d4)) / ($d1 + $d2 + $d3 + $d4);
$Nc = 0;
$Vc = $f2*$d3 + $f1 - $Ay;
$Mc = abs($Ay*($d1+$d2+$d3) - $f1*($d2+$d3) - $f2 * ($d3) * ($d3/2));

